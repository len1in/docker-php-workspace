[mysqld]
skip-log-bin
skip-host-cache
skip-name-resolve

innodb_doublewrite = 0

innodb_flush_log_at_trx_commit = 2
innodb_file_per_table = 1
query_cache_size = 0
innodb_flush_method = fsync
innodb_use_native_aio = false
lower_case_table_names = 2

# Off strict mode
sql_mode=""

# Slow query settings:
slow_query_log=0
slow_query_log_file=/var/log/mysql/slow.log
long_query_time=10